<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="intecap.gt.proyecto_belleza2016">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Navigation_menu"
            android:label="@string/title_activity_navigation_menu"
            android:theme="@style/AppTheme" />
        <activity android:name=".view.Registro" />
        <activity android:name=".view.DragNDropTiposActivity" />
        <activity android:name=".view.DragNDrop" />
        <activity android:name=".Video" />
        <activity android:name=".view.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.Login" />
        <activity
            android:name=".prueba.DesarrolloActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="intecap.gt.proyecto_belleza2016.prueba.Presentacion" />
        </activity>
        <activity android:name=".view.BibliografiaActivity"></activity>
    </application>

</manifest>